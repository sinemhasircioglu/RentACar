<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <!-- Fonts -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800,400italic%7cMontserrat:400,700" rel="stylesheet" type="text/css">
    <!-- Icons -->
    <link href="~/Areas/admin/Content/typicons.min.css" rel="stylesheet" />
    <link href="~/Areas/admin/Content/font-awesome.min.css" rel="stylesheet" />
    <!-- iCheck - Custom radio/check input -->
    <link href="~/Areas/admin/Content/icheck/minimal.css" rel="stylesheet" />
    <!-- Bootstrap -->
    <link href="~/Areas/admin/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Footable plugin -->
    <link href="~/Areas/admin/Content/plugins.min.css" rel="stylesheet" />
    <!-- Template -->
    <link href="~/Areas/admin/Content/main.css" rel="stylesheet" />
    <link href="~/Areas/admin/Content/bootstrap-tagsinput.css" rel="stylesheet" />
    <!--[if lt IE 9]>
      <script src="js/plugins/shiv.min.js"></script>
      <script src="js/plugins/respond.min.js"></script>
      <script src="js/plugins/excanvas.min.js"></script>
    <![endif]-->
    <script src="~/Areas/admin/Scripts/plugins/jquery.min.js"></script>
</head>
<body>
    <!-- Page container -->
    <div class="page-container">
        <!-- Sidebar -->
        @Html.Partial("_Sidebar")
        <!-- /Sidebar -->
        <!-- Content -->
        <div id="content">
            <!-- Header -->
            @Html.Partial("_HeaderAdmin")
            <!-- /Header -->
            <!-- Content start -->
            @RenderBody()
        </div>
        <!-- /Content -->
    </div>
    <!-- /Page container -->
    <!-- PLUGINS ALL -->
    <script src="~/Areas/admin/Scripts/plugins/modernizr.min.js"></script>
    <!-- Bootstrap core -->
    <script src="~/Areas/admin/Scripts/plugins/bootstrap.min.js"></script>
    <!-- Custom scrollbar -->
    <script src="~/Areas/admin/Scripts/plugins/enscroll.min.js"></script>
    <!-- Dynamic tables -->
    <script src="~/Areas/admin/Scripts/plugins/footable.min.js"></script>
    <!-- Knob / Donut charts -->
    <script src="~/Areas/admin/Scripts/plugins/knob.min.js"></script>
    <!-- Rickshaw graphs & charts -->
    <script src="~/Areas/admin/Scripts/plugins/sparkline.min.js"></script>
    <script src="~/Areas/admin/Scripts/plugins/rickshaw/d3.v3.js"></script>
    <script src="~/Areas/admin/Scripts/plugins/rickshaw/rickshaw.min.js"></script>
    <!-- Flot -->
    <script src="~/Areas/admin/Scripts/plugins/flot.min.js"></script>
    <!-- Custom checkboxes and radio input -->
    <script src="~/Areas/admin/Scripts/plugins/icheck.min.js"></script>
    <!-- Site -->
    <script src="~/Areas/admin/Scripts/main.js"></script>
    <script src="~/Areas/admin/Scripts/bootstrap-tagsinput.js"></script>
</body>
</html>